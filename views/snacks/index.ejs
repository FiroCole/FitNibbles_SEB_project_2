<%- include('../partials/header') %>


<table id="list">
    <thead>
      <tr>
        <th>Snack</th>
      </tr>
    </thead>
    <tbody>
      <% snacks.forEach(function(s) { %>
        <tr>
          <td><%= s.Snack %></td>
          <td><a href="/snacks/<%= s._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
<%- include('../partials/footer') %>